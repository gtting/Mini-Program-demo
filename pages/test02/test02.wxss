.container{
  display: flex;
  flex-direction: column;
}
.c_item{
  flex: 1;
  margin:10px 0;
  padding: 10px;
  line-height: 32px;
  background-color: #fff;
  justify-content: space-between;
  display: flex;
}
.t_img{
  width: 50px;
  height: 50px;
  border: 1px solid #666;
  margin-right: 10px;
}
.t_center{
  font-size: 16px;
