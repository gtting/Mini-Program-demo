  margin-left:20px;
}
.m0{
  margin: 0;
  border-bottom: 1px solid #f7f7f7;
}
.bottom-bar{
  display: flex;
  justify-content: space-around;
  border-top:1px solid #ddd;
  padding: 10px 0;
  font-size: 12px;
  color: #999;
  background-color: #fff;
  position: fixed;
  bottom:0;
  width: 100%;
  text-align: center;
}